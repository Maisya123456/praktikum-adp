from termcolor import colored,cprint

for i in range (1):
    cprint (" "*13, "white", end =" ")
    cprint(" ", end= " ")
    cprint (" "*1, "white", "on_white", end=" ")
    cprint(" "*1, "white", "on_yellow", end=" ")
    cprint(" "*1, "white", "on_white")
    
for i in range (1):
    cprint (" "*11, "white", end =" ")
    cprint(" ", end= " ")
    cprint (" "*9, "white", "on_magenta")

